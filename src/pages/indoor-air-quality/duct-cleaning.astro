	<!-- Our Duct Cleaning Process -->
	<section class="py-16 md:py-24 bg-white">
		<div class="container mx-auto px-4">
			<div class="text-center mb-12">
				<div class="flex items-center justify-center mb-4">
					<div class="w-10 h-1 bg-accent mr-4"></div>
					<h2 class="text-3xl font-bold text-primary font-heading">OUR DUCT CLEANING PROCESS</h2>
					<div class="w-10 h-1 bg-accent ml-4"></div>
				</div>
				<p class="text-lg text-darkgray max-w-3xl mx-auto">
					We follow a comprehensive, step-by-step process to ensure thorough cleaning of your entire duct system.
				</p>
			</div>

			<div class="grid grid-cols-1 md:grid-cols-2 gap-12 items-center">
				<div>
					<ol class="space-y-8">
						<li class="flex">
							<div
								class="flex-shrink-0 w-10 h-10 rounded-full bg-secondary text-white flex items-center justify-center mr-4 font-bold">
								1
							</div>
							<div>
								<h3 class="text-xl font-bold text-primary mb-2">Initial Inspection</h3>
								<p class="text-darkgray">
									We begin with a thorough inspection of your ductwork to assess the level of contamination and identify
									any potential issues that may need to be addressed.
								</p>
							</div>
						</li>
						<li class="flex">
							<div
								class="flex-shrink-0 w-10 h-10 rounded-full bg-secondary text-white flex items-center justify-center mr-4 font-bold">
								2
							</div>
							<div>
								<h3 class="text-xl font-bold text-primary mb-2">System Protection</h3>
								<p class="text-darkgray">
									Before cleaning begins, we take steps to protect your home and HVAC system, including covering
									furniture and flooring near vents and sealing off areas as needed.
								</p>
							</div>
						</li>
						<li class="flex">
							<div
								class="flex-shrink-0 w-10 h-10 rounded-full bg-secondary text-white flex items-center justify-center mr-4 font-bold">
								3
							</div>
