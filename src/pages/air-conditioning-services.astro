---
import '../styles/global.css';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import ServiceProcess from '../components/ServiceProcess.astro';
import ComfortCare from '../components/ComfortCare.astro';
import GoogleHead from "../components/GoogleHead.astro";
import PagesCTA from "../components/PagesCTA.astro";
import settings from "../settings";

const ldLocalBusiness = {
	"@context": "https://schema.org",
	"@type": "Service",
	"name": "Air Conditioning Services",
	"provider": {
		"@type": "HVACBusiness",
		"name": "Alltek Services",
		"address": {
			"@type": "PostalAddress",
			"streetAddress": "1181 FM-2271 Hwy",
			"addressLocality": "Belton",
			"addressRegion": "TX",
			"postalCode": "76513",
			"addressCountry": "US"
		},
		"telephone": settings.phoneMainRaw,
		"image": "/images/Logo.png",
		"priceRange": "$$"
	},
	"areaServed": {
		"@type": "City",
		"name": "Belton",
		"sameAs": "https://en.wikipedia.org/wiki/Belton,_Texas"
	},
	"serviceType": "Air Conditioning Services",
	"description": "Professional air conditioning services including repair, installation, maintenance, and emergency services for residential and commercial properties in Belton and Central Texas.",
	"offers": {
		"@type": "Offer",
		"availability": "https://schema.org/InStock",
		"priceSpecification": {
			"@type": "PriceSpecification",
			"priceCurrency": "USD"
		}
	}
};
---

<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8"/>
	<link rel="icon" type="image/png" href="/images/Logo.png"/>
	<meta name="viewport" content="width=device-width"/>
	<meta name="generator" content={Astro.generator}/>

	<!-- Primary Meta Tags -->
	<title>Air Conditioning Services in Belton, TX | Alltek Services</title>
	<meta name="title" content="Air Conditioning Services in Belton, TX | Alltek Services"/>
	<meta name="description"
	      content="Professional air conditioning services in Belton, TX including AC repair, installation, maintenance, and emergency services. Keep your home cool and comfortable year-round."/>
	<meta name="keywords"
	      content="air conditioning services Belton TX, AC repair, AC installation, AC maintenance, emergency AC repair, ductless mini split, air conditioning refrigerant, HVAC cooling services, Central Texas AC services"/>

	<!-- Open Graph / Facebook -->
	<meta property="og:type" content="website"/>
	<meta property="og:url" content="https://www.alltek-services.com/air-conditioning-services/"/>
	<meta property="og:title" content="Air Conditioning Services in Belton, TX | Alltek Services"/>
	<meta property="og:description"
	      content="Professional air conditioning services in Belton, TX including AC repair, installation, maintenance, and emergency services. Keep your home cool and comfortable year-round."/>
	<meta property="og:image" content="https://www.alltek-services.com/images/services/air-conditioning.jpg"/>

	<!-- Twitter -->
	<meta property="twitter:card" content="summary_large_image"/>
	<meta property="twitter:url" content="https://www.alltek-services.com/air-conditioning-services/"/>
	<meta property="twitter:title" content="Air Conditioning Services in Belton, TX | Alltek Services"/>
	<meta property="twitter:description"
	      content="Professional air conditioning services in Belton, TX including AC repair, installation, maintenance, and emergency services. Keep your home cool and comfortable year-round."/>
	<meta property="twitter:image" content="https://www.alltek-services.com/images/services/air-conditioning.jpg"/>

	<!-- Canonical URL -->
	<link rel="canonical" href="https://www.alltek-services.com/air-conditioning-services/"/>

	<!-- Google Fonts -->
	<link rel="preconnect" href="https://fonts.googleapis.com">
	<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
	<link
		href="https://fonts.googleapis.com/css2?family=Montserrat:wght@400;500;600;700&family=Open+Sans:wght@400;500;600;700&display=swap"
		rel="stylesheet">

	<!-- Structured Data for Service -->
	<script type="application/ld+json" set:html={JSON.stringify(ldLocalBusiness)}/>

	<!-- Breadcrumb Schema -->
	<script type="application/ld+json">
		{
			"@context": "https://schema.org",
			"@type": "BreadcrumbList",
			"itemListElement": [
				{
					"@type": "ListItem",
					"position": 1,
					"name": "Home",
					"item": "https://www.alltek-services.com/"
				},
				{
					"@type": "ListItem",
					"position": 2,
					"name": "Services",
					"item": "https://www.alltek-services.com/services/"
				},
				{
					"@type": "ListItem",
					"position": 3,
					"name": "Air Conditioning Services",
					"item": "https://www.alltek-services.com/air-conditioning-services/"
				}
			]
		}
	</script>
	<GoogleHead/>
</head>
<body class="bg-white">
<Header/>

<!-- Breadcrumbs -->
<div class="bg-lightgray py-3">
	<div class="container mx-auto px-4">
		<nav class="flex" aria-label="Breadcrumb">
			<ol class="inline-flex items-center space-x-1 md:space-x-3">
				<li class="inline-flex items-center">
					<a href="/" class="text-darkgray hover:text-accent inline-flex items-center">
						<svg class="w-4 h-4 mr-2" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
							<path
								d="M10.707 2.293a1 1 0 00-1.414 0l-7 7a1 1 0 001.414 1.414L4 10.414V17a1 1 0 001 1h2a1 1 0 001-1v-2a1 1 0 011-1h2a1 1 0 011 1v2a1 1 0 001 1h2a1 1 0 001-1v-6.586l.293.293a1 1 0 001.414-1.414l-7-7z"></path>
						</svg>
						Home
					</a>
				</li>
				<li>
					<div class="flex items-center">
						<svg class="w-6 h-6 text-darkgray" fill="currentColor" viewBox="0 0 20 20"
						     xmlns="http://www.w3.org/2000/svg">
							<path fill-rule="evenodd"
							      d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z"
							      clip-rule="evenodd"></path>
						</svg>
						<a href="/services" class="text-darkgray hover:text-accent ml-1 md:ml-2">Services</a>
					</div>
				</li>
				<li aria-current="page">
					<div class="flex items-center">
						<svg class="w-6 h-6 text-darkgray" fill="currentColor" viewBox="0 0 20 20"
						     xmlns="http://www.w3.org/2000/svg">
							<path fill-rule="evenodd"
							      d="M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z"
							      clip-rule="evenodd"></path>
						</svg>
						<span class="text-accent font-medium ml-1 md:ml-2">Air Conditioning Services</span>
					</div>
				</li>
			</ol>
		</nav>
	</div>
</div>

<main>
	<!-- Hero Section -->
	<section class="py-16 md:py-24 bg-primary text-white">
		<div class="container mx-auto px-4">
			<div class="text-center max-w-4xl mx-auto">
				<h1 class="text-4xl md:text-5xl font-bold mb-6 font-heading">AIR CONDITIONING SERVICES</h1>
				<p class="text-xl mb-8">Keep your home cool and comfortable during the hot Texas summers with our comprehensive
					air conditioning services.</p>
			</div>
		</div>
	</section>

	<!-- Service Overview -->
	<section class="py-16 md:py-24 bg-white">
		<div class="container mx-auto px-4">
			<div class="grid grid-cols-1 md:grid-cols-2 gap-12 items-center">
				<div>
					<div class="flex items-center mb-4">
						<div class="w-10 h-1 bg-accent mr-4"></div>
						<h2 class="text-3xl font-bold text-primary font-heading">EXPERT AC SERVICES</h2>
					</div>
					<h3 class="text-xl font-semibold text-darkgray mb-6">
						KEEPING CENTRAL TEXAS COOL SINCE 2010
					</h3>
					<p class="text-darkgray mb-4">
						At Alltek Services, we understand the importance of a reliable air conditioning system in the Texas heat.
						Our team of certified technicians provides comprehensive air conditioning services to ensure your home or
						business stays cool and comfortable year-round.
					</p>
					<p class="text-darkgray mb-4">
						From routine maintenance to emergency repairs and new installations, we have the expertise and equipment to
						handle all your air conditioning needs. We work with all major brands and models, providing prompt,
						professional service at competitive rates.
					</p>
					<p class="text-darkgray mb-8">
						Our commitment to quality workmanship and customer satisfaction has made us a trusted name in Belton and
						throughout Central Texas. When you choose Alltek Services, you can rest assured that your air conditioning
						system is in good hands.
					</p>
					<a
						href="/contact"
						class="inline-block bg-secondary hover:bg-secondary/90 text-white font-bold py-3 px-8 rounded-md transition-colors"
					>
						Get Free Estimate
					</a>
				</div>

				<div>
					<img
						src="/images/ac-servicing_600w.jpg"
						alt="HVAC technician performing air conditioning maintenance"
						class="rounded-lg shadow-xl w-full h-auto object-cover"
						width="600"
						height="400"
					/>
				</div>
			</div>
		</div>
	</section>

	<!-- Services List -->
	<section class="py-16 md:py-24 bg-lightgray">
		<div class="container mx-auto px-4">
			<div class="text-center mb-12">
				<div class="flex items-center justify-center mb-4">
					<div class="w-10 h-1 bg-accent mr-4"></div>
					<h2 class="text-3xl font-bold text-primary font-heading">OUR AC SERVICES</h2>
					<div class="w-10 h-1 bg-accent ml-4"></div>
				</div>
				<p class="text-lg text-darkgray max-w-3xl mx-auto">
					We offer a complete range of air conditioning services to meet all your cooling needs.
				</p>
			</div>

			<div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-8">
				<!-- AC Repair -->
				<div class="bg-white p-8 rounded-lg shadow-md">
					<div class="w-16 h-16 bg-secondary rounded-full flex items-center justify-center mb-6 mx-auto">
						<svg xmlns="http://www.w3.org/2000/svg" class="h-8 w-8 text-white" fill="none" viewBox="0 0 24 24"
						     stroke="currentColor">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
							      d="M12 6V4m0 2a2 2 0 100 4m0-4a2 2 0 110 4m-6 8a2 2 0 100-4m0 4a2 2 0 110-4m0 4v2m0-6V4m6 6v10m6-2a2 2 0 100-4m0 4a2 2 0 110-4m0 4v2m0-6V4"/>
						</svg>
					</div>
					<h3 class="text-xl font-bold text-primary mb-4 text-center">Air Conditioning Repair</h3>
					<p class="text-darkgray mb-4 text-center">
						Our skilled technicians can diagnose and repair any air conditioning issue quickly and efficiently. We
						service all makes and models.
					</p>
					<ul class="space-y-2 text-darkgray mb-6">
						<li class="flex items-start">
							<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-accent mr-2 mt-0.5 flex-shrink-0"
							     viewBox="0 0 20 20" fill="currentColor">
								<path fill-rule="evenodd"
								      d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z"
								      clip-rule="evenodd"/>
							</svg>
							Zone-specific temperature control
						</li>
						<li class="flex items-start">
							<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-accent mr-2 mt-0.5 flex-shrink-0"
							     viewBox="0 0 20 20" fill="currentColor">
								<path fill-rule="evenodd"
								      d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z"
								      clip-rule="evenodd"/>
							</svg>
							No ductwork required
						</li>
						<li class="flex items-start">
							<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-accent mr-2 mt-0.5 flex-shrink-0"
							     viewBox="0 0 20 20" fill="currentColor">
								<path fill-rule="evenodd"
								      d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z"
								      clip-rule="evenodd"/>
							</svg>
							Easy installation
						</li>
					</ul>
					<div class="text-center">
						<a href="/air-conditioning-services/ductless-mini-split"
						   class="text-secondary hover:text-accent font-bold transition-colors">
							Learn More →
						</a>
					</div>
				</div>

				<!-- Emergency AC Repair -->
				<div class="bg-white p-8 rounded-lg shadow-md">
					<div class="w-16 h-16 bg-secondary rounded-full flex items-center justify-center mb-6 mx-auto">
						<svg xmlns="http://www.w3.org/2000/svg" class="h-8 w-8 text-white" fill="none" viewBox="0 0 24 24"
						     stroke="currentColor">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
							      d="M12 8v4m0 4h.01M21 12a9 9 0 11-18 0 9 9 0 0118 0z"/>
						</svg>
					</div>
					<h3 class="text-xl font-bold text-primary mb-4 text-center">Emergency AC Repair</h3>
					<p class="text-darkgray mb-4 text-center">
						When your AC fails during the hottest days, our emergency repair service is available to restore your
						comfort quickly.
					</p>
					<ul class="space-y-2 text-darkgray mb-6">
						<li class="flex items-start">
							<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-accent mr-2 mt-0.5 flex-shrink-0"
							     viewBox="0 0 20 20" fill="currentColor">
								<path fill-rule="evenodd"
								      d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z"
								      clip-rule="evenodd"/>
							</svg>
							Rapid response time
						</li>
						<li class="flex items-start">
							<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-accent mr-2 mt-0.5 flex-shrink-0"
							     viewBox="0 0 20 20" fill="currentColor">
								<path fill-rule="evenodd"
								      d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z"
								      clip-rule="evenodd"/>
							</svg>
							Fully stocked service vehicles
						</li>
						<li class="flex items-start">
							<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-accent mr-2 mt-0.5 flex-shrink-0"
							     viewBox="0 0 20 20" fill="currentColor">
								<path fill-rule="evenodd"
								      d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z"
								      clip-rule="evenodd"/>
							</svg>
							Same-day service (during business hours)
						</li>
					</ul>
					<div class="text-center">
						<a href="/air-conditioning-services/emergency-ac-repair"
						   class="text-secondary hover:text-accent font-bold transition-colors">
							Learn More →
						</a>
					</div>
				</div>

				<!-- AC Refrigerant -->
				<div class="bg-white p-8 rounded-lg shadow-md">
					<div class="w-16 h-16 bg-secondary rounded-full flex items-center justify-center mb-6 mx-auto">
						<svg xmlns="http://www.w3.org/2000/svg" class="h-8 w-8 text-white" fill="none" viewBox="0 0 24 24"
						     stroke="currentColor">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
							      d="M19.428 15.428a2 2 0 00-1.022-.547l-2.387-.477a6 6 0 00-3.86.517l-.318.158a6 6 0 01-3.86.517L6.05 15.21a2 2 0 00-1.806.547M8 4h8l-1 1v5.172a2 2 0 00.586 1.414l5 5c1.26 1.26.367 3.414-1.415 3.414H4.828c-1.782 0-2.674-2.154-1.414-3.414l5-5A2 2 0 009 10.172V5L8 4z"/>
						</svg>
					</div>
					<h3 class="text-xl font-bold text-primary mb-4 text-center">Air Conditioning Refrigerant</h3>
					<p class="text-darkgray mb-4 text-center">
						We provide professional refrigerant services, including leak detection, repair, and environmentally
						responsible recharging.
					</p>
					<ul class="space-y-2 text-darkgray mb-6">
						<li class="flex items-start">
							<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-accent mr-2 mt-0.5 flex-shrink-0"
							     viewBox="0 0 20 20" fill="currentColor">
								<path fill-rule="evenodd"
								      d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z"
								      clip-rule="evenodd"/>
							</svg>
							Refrigerant leak detection
						</li>
						<li class="flex items-start">
							<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-accent mr-2 mt-0.5 flex-shrink-0"
							     viewBox="0 0 20 20" fill="currentColor">
								<path fill-rule="evenodd"
								      d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z"
								      clip-rule="evenodd"/>
							</svg>
							EPA-compliant refrigerant handling
						</li>
						<li class="flex items-start">
							<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-accent mr-2 mt-0.5 flex-shrink-0"
							     viewBox="0 0 20 20" fill="currentColor">
								<path fill-rule="evenodd"
								      d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z"
								      clip-rule="evenodd"/>
							</svg>
							System recharging
						</li>
						<li class="flex items-start">
							<svg xmlns="http://www.w3.org/2000/svg" class="h-5 w-5 text-accent mr-2 mt-0.5 flex-shrink-0"
							     viewBox="0 0 20 20" fill="currentColor">
								<path fill-rule="evenodd"
								      d="M16.707 5.293a1 1 0 010 1.414l-8 8a1 1 0 01-1.414 0l-4-4a1 1 0 011.414-1.414L8 12.586l7.293-7.293a1 1 0 011.414 0z"
								      clip-rule="evenodd"/>
							</svg>
							R-22 to R-410A conversions
						</li>
					</ul>
					<div class="text-center">
						<a href="/air-conditioning-services/air-conditioning-refrigerant"
						   class="text-secondary hover:text-accent font-bold transition-colors">
							Learn More →
						</a>
					</div>
				</div>
			</div>
		</div>
	</section>

	<!-- Why Choose Us -->
	<section class="py-16 md:py-24 bg-white">
		<div class="container mx-auto px-4">
			<div class="text-center mb-12">
				<div class="flex items-center justify-center mb-4">
					<div class="w-10 h-1 bg-accent mr-4"></div>
					<h2 class="text-3xl font-bold text-primary font-heading">WHY CHOOSE US</h2>
					<div class="w-10 h-1 bg-accent ml-4"></div>
				</div>
				<p class="text-lg text-darkgray max-w-3xl mx-auto">
					When you choose Alltek Services for your air conditioning needs, you're choosing quality, reliability, and
					exceptional customer service.
				</p>
			</div>

			<div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-8">
				<div class="bg-lightgray p-6 rounded-lg text-center">
					<div class="w-16 h-16 bg-secondary rounded-full flex items-center justify-center mb-4 mx-auto">
						<svg xmlns="http://www.w3.org/2000/svg" class="h-8 w-8 text-white" fill="none" viewBox="0 0 24 24"
						     stroke="currentColor">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
							      d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z"/>
						</svg>
					</div>
					<h3 class="text-xl font-bold text-primary mb-2">Licensed & Insured</h3>
					<p class="text-darkgray">
						Our technicians are fully licensed, insured, and certified to provide the highest quality service.
					</p>
				</div>

				<div class="bg-lightgray p-6 rounded-lg text-center">
					<div class="w-16 h-16 bg-secondary rounded-full flex items-center justify-center mb-4 mx-auto">
						<svg xmlns="http://www.w3.org/2000/svg" class="h-8 w-8 text-white" fill="none" viewBox="0 0 24 24"
						     stroke="currentColor">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
							      d="M12 8v4l3 3m6-3a9 9 0 11-18 0 9 9 0 0118 0z"/>
						</svg>
					</div>
					<h3 class="text-xl font-bold text-primary mb-2">24/7 Emergency Service</h3>
					<p class="text-darkgray">
						We're available around the clock to handle any air conditioning emergency that may arise.
					</p>
				</div>

				<div class="bg-lightgray p-6 rounded-lg text-center">
					<div class="w-16 h-16 bg-secondary rounded-full flex items-center justify-center mb-4 mx-auto">
						<svg xmlns="http://www.w3.org/2000/svg" class="h-8 w-8 text-white" fill="none" viewBox="0 0 24 24"
						     stroke="currentColor">
							<path stroke-linecap="round" stroke-linejoin="round" stroke-width="2"
							      d="M17 9V7a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2m2 4h10a2 2 0 002-2v-6a2 2 0 00-2-2H9a2 2 0 00-2 2v6a2 2 0 002 2zm7-5a2 2 0 11-4 0 2 2 0 014 0z"/>
						</svg>
					</div>
					<h3 class="text-xl font-bold text-primary mb-2">Competitive Pricing</h3>
					<p class="text-darkgray">
						We offer fair, transparent pricing with no hidden fees or surprise charges.
					</p>
